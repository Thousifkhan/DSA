package StudentProduct;

public class Student {
	
	int age;
	String name;
	String place;
	String mobileno;
	String country;
	
		private Student(Helper helper) throws Exception {
			
			if(helper.getAge() < 15 || helper.getAge() > 80) {
				
				throw new IllegalAccessException("AGE IS NOT CORRECT");
			}
			this.name = helper.getName();
			this.age = helper.getAge();
		}

	
	
	
	

}
